.v-section{
  background:#fff;
  border:1rpx solid #eee;
  border-width :1rpx 0;
}
.v-line{
  height :20rpx;
}
.v-cell{
  padding:28rpx;
  position :relative;
  display :flex;
  flex-direction: row;
  align-items: center;
  font-size :28rpx;
}
.v-cell.baseline{
  align-items :baseline
}
.v-cell .span{
  flex :1;
}
.v-cell .b{
  font-weight :bold
}
.v-cell.around{
  justify-content :space-around;
}
.v-cell.between{
  justify-content :space-between;
}
.v-cell:after{
  height :1rpx;
  background :#eee;
  position :absolute;
  right :0;
  bottom :0;
  left :28rpx;
  content: "";
}
.v-cell .left-icon{
  margin-right :10rpx;
}
.v-box{
  display :flex;
  flex-direction: row;
  align-items: center;
}
.v-grid{
  display :flex;
  flex-direction: row;
  align-items: center;
  /*justify-content: space-between;*/
  justify-content :space-around;
}
.v-grid-item{
  padding :28rpx; text-align :center;
  font-size :20rpx;
}
.v-grid-item i{
  display :block; font-size :42rpx;
}
.v-title{
  font-size :20rpx; color :#a6a6a6; padding: 0 0 10rpx 28rpx;
}
.v-nav{
  position :relative;
}
.v-nav:after{
  font-family:"iconfont";
  content: "\e6a7";
  right :28rpx;
  top:28rpx;
  position :absolute;
}
.v-cell.icon{

}

.btn-spacing{
  padding :20rpx
}

.space-between{
  justify-content: space-between;
}

.v-input {
  min-width:200rpx;
  margin-left:15rpx;
  height:2.58823529em;
  min-height:2.58823529em;
  line-height:2.58823529em;
}
.userBase{
  position :relative;
}
.userBase .vip{
  position :absolute;
  top :20rpx; right :20rpx;
  color :#fff;
}

text.btn,contact-button.btn{
  background :#eee; border-radius :2px; padding :0 10rpx; line-height :60rpx;  font-size :25rpx; display :inline-block;

}
text.btn.on:hover,contact-button.btn:hover{
  background :#222;color :#fff;
}

.button-hover[type="primary"] {
color:#eee;
background-color:#333;

}
